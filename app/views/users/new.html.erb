<h1>New User</h1>


<%= form_tag(users_url, method: 'post') do %>


  <div>
    <%= label_tag :username %><br />
    <%= text_field_tag :username %>
  </div>

    <div>
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
  </div>

    <div>
    <%= label_tag :bio %><br />
    <%= text_area_tag :bio %>
  </div>

  
  <div>
    <%= submit_tag "Create User" %>
  </div>
<% end %>
